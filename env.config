# Data volume root dir
export VOLUME_PATH=/srv/nexus-data

# will be accessable with https://SERVER_NAME
export SERVER_NAME=nexus.healbe.com

# docker Public repo with anonymous push/pull
export DOCKER_DEV_NAME=nexus-pub.healbe.com

# docker virtual repo with anonymous pull
export DOCKER_VIRTUAL_NAME=nexus-virtual.healbe.com

# will be used in docker-proxy upstream configuration
export UPSTREAM_DOCKER_REPO=https://nexus-prod-virtual.healbe.com
